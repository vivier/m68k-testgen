char *getccr(uint16 ccr)
{
 static char text[6];

 text[0]=(ccr & X_FLAG) ? 'X':'.';
 text[1]=(ccr & N_FLAG) ? 'N':'.';
 text[2]=(ccr & Z_FLAG) ? 'Z':'.';
 text[3]=(ccr & V_FLAG) ? 'V':'.';
 text[4]=(ccr & C_FLAG) ? 'C':'.';

 text[5]=0; // terminate string

 return text;
}

